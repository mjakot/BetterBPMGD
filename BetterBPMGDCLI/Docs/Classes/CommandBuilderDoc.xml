  <method name="AddOptionSimple">
    <summary>
      Adds an option to the command
    </summary>
    <typeparam name="OptionType">
      Specifies the option type
    </typeparam>
    <param name="isRequired">
      Specifies whether an option is required. Default is <see langword="true"/>
    </param>
    <returns>
      <see cref="BetterBPMGDCLI.CLICommands.CommandBuilder{T}"/>
    </returns>
  </method>

  <method name="AddOptionFull">
    <summary>
      Adds an option to the command
    </summary>
    <typeparam name="OptionType">
      Specifies the option type
    </typeparam>
    <param name="isRequired">
      Specifies whether an option is required. Default is <see langword="true"/>
    </param>
    <param name="validator">
      Specifies the validator for the argument. Set to <see langword="null"/> to skip
    </param>
    <param name="getDefaultValue">
      Specifies the default value for the argument. Set to <see langword="null"/> to skip
    </param>
    <param name="fromAmong">
      Specifies the array of choices for the argument. Set to <see langword="[]"/> to skip
    </param>
    <returns>
      <see cref="BetterBPMGDCLI.CLICommands.CommandBuilder{T}"/>
    </returns>
  </method>

